{
  "name": "@temir/core",
  "type": "module",
  "version": "0.0.2",
  "description": "Vue for CLI",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/temir#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/temir.git"
  },
  "keywords": [
    "vue",
    "cli",
    "stdout",
    "components",
    "command-line",
    "print",
    "render",
    "colors",
    "text"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.cjs",
    "dist/**/*.mjs",
    "dist/**/*.d.ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.37",
    "ansi-escapes": "^4.3.2",
    "auto-bind": "4.0.0",
    "chalk": "^4.1.2",
    "cli-boxes": "^2.2.1",
    "cli-cursor": "^3.1.0",
    "cli-truncate": "^2.1.0",
    "indent-string": "^4.0.0",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "patch-console": "^1.0.0",
    "signal-exit": "^3.0.7",
    "slice-ansi": "^4.0.0",
    "string-width": "4.2.3",
    "vue": "^3.2.37",
    "widest-line": "3.1.0",
    "wrap-ansi": "^7.0.0",
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "devDependencies": {
    "@types/is-ci": "^3.0.0",
    "@types/lodash": "^4.14.182",
    "@types/signal-exit": "^3.0.1",
    "tsup": "^6.2.1"
  }
}
