{
  "name": "@temir/select-input",
  "type": "module",
  "version": "0.0.15",
  "description": "Select Input component for Temir",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/temir#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/temir.git"
  },
  "keywords": [
    "vue",
    "cli",
    "stdout",
    "components",
    "command-line",
    "print",
    "render",
    "colors",
    "text",
    "select-input"
  ],
  "exports": {
