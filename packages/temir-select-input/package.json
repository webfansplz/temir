  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup"
  },
  "dependencies": {
    "@temir/core": "workspace:*",
    "vue": "^3.2.37",
    "arr-rotate": "^1.0.0",
    "figures": "^3.2.0"
  },
  "devDependencies": {
    "tsup": "^6.2.1"
  }
